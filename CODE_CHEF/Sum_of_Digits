#include<iostream>
using namespace std;
int main(){
    int t;
    cin>>t;
    int arr[t];
    for(int i=0;i<t;i++){
        cin>>arr[i];
    }
    for(int i=0;i<t;i++){
        int res=0;
        while(arr[i]!=0){
            res+=arr[i]%10;
            arr[i]/=10;
        }
        cout<<res<<"\n";
    }
}
